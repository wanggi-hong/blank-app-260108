{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Streamlit",
      "type": "python",
      "request": "launch",
      "module": "streamlit",
      "args": [
        "run",
        "${workspaceFolder}/streamlit_app.py",
        "--server.enableCORS",
        "false",
        "--server.enableXsrfProtection",
        "false"
      ],
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
